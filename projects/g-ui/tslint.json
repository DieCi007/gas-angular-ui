{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "no-string-literal": false,
    "component-selector": [
      true,
      "element",
      "g",
      "kebab-case"
    ],
    "variable-name": {
      "options": [
        "ban-keywords",
        "allow-pascal-case"
      ]
    }
  }
}
